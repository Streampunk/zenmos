{
  "name": "zenmos",
  "version": "0.0.1",
  "description": "Automated testing application for AMWA NMOS IS-04.",
  "main": "./dist/electron/main.js",
  "scripts": {
    "build": "node .electron-vue/build.js",
    "build:darwin": "cross-env BUILD_TARGET=darwin node .electron-vue/build.js",
    "build:linux": "cross-env BUILD_TARGET=linux node .electron-vue/build.js",
    "build:mas": "cross-env BUILD_TARGET=mas node .electron-vue/build.js",
    "build:win32": "cross-env BUILD_TARGET=win32 node .electron-vue/build.js",
    "build:clean": "cross-env BUILD_TARGET=clean node .electron-vue/build.js",
    "build:web": "cross-env BUILD_TARGET=web node .electron-vue/build.js",
    "dev": "node .electron-vue/dev-runner.js",
    "test": "tape test/*.js",
    "lint": "eslint **/*.js",
    "lint:fix": "eslint --ext .js,.vue -f ./node_modules/eslint-friendly-formatter --fix src",
    "pack": "npm run pack:main && npm run pack:renderer",
    "pack:main": "cross-env NODE_ENV=production webpack --progress --colors --config .electron-vue/webpack.main.config.js",
    "pack:renderer": "cross-env NODE_ENV=production webpack --progress --colors --config .electron-vue/webpack.renderer.config.js",
    "postinstall": "npm run lint:fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Streampunk/zenmos.git"
  },
  "keywords": [
    "NMOS",
    "AMWA",
    "IS-04"
  ],
  "author": "Streampunk Media <furnace@streampunk.media> (http://www.streampunk.media)",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/Streampunk/zenmos/issues"
  },
  "homepage": "https://github.com/Streampunk/zenmos#readme",
  "dependencies": {
    "ajv": "^6.1.1",
    "body-parser": "^1.18.2",
    "digd.js": "git+https://git.coolaj86.com/scriptorian/digd.js#master",
    "dns-suite": "git+https://git.coolaj86.com/scriptorian/dns-suite.js#master",
    "express": "^4.16.2",
    "node-red": "^0.18.4",
    "request": "^2.83.0",
    "request-promise-native": "^1.0.5",
    "uuid": "^3.2.1",
    "vue": "^2.5.15",
    "vue-electron": "^1.0.6",
    "vue-router": "^3.0.1",
    "vuetify": "1.0.6",
    "yargs": "^11.0.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "babili-webpack-plugin": "^0.1.2",
    "cfonts": "^1.1.3",
    "chalk": "^2.3.0",
    "copy-webpack-plugin": "^4.3.1",
    "cross-env": "^5.1.3",
    "css-loader": "^0.28.9",
    "del": "^3.0.0",
    "devtron": "^1.4.0",
    "electron": "^1.7.11",
    "electron-debug": "^1.5.0",
    "electron-devtools-installer": "^2.2.3",
    "electron-packager": "^10.1.1",
    "electron-rebuild": "^1.7.3",
    "babel-eslint": "^8.2.1",
    "eslint": "^4.18.2",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-html": "^4.0.2",
    "eslint-config-google": "^0.9.1",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-node": "^5.2.1",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-standard": "^3.0.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.6",
    "html-webpack-plugin": "^2.30.1",
    "multispinner": "^0.2.1",
    "node-loader": "^0.6.0",
    "style-loader": "^0.19.1",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.1",
    "tape": "^4.9.0",
    "url-loader": "^0.6.2",
    "vue-html-loader": "^1.2.4",
    "vue-loader": "^13.7.0",
    "vue-style-loader": "^3.1.1",
    "vue-template-compiler": "^2.5.13",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.11.1",
    "webpack-hot-middleware": "^2.21.0"
  },
  "node-red": {
    "nodes": {
      "jsonValidator": "nodes/jsonValidator.js",
      "auditLog": "nodes/auditLog.js",
      "stateStoreRAM": "nodes/stateStoreRAM.js",
      "registrationLogic": "nodes/registrationLogic.js",
      "queryLogic": "nodes/queryLogic.js",
      "nmosHTTPRequest": "nodes/nmosHTTPRequest.js",
      "mdns": "nodes/mdns.js",
      "dns-sd": "nodes/dns-sd.js",
      "dnsBrowser": "nodes/dnsBrowser.js",
      "nodeLogid": "nodes/nodeLogic.js",
      "heartbeats": "nodes/heartbeats.js",
      "nodeRequester": "nodes/nodeRequester.js"
    }
  }
}
