{
  "name": "zenmos",
  "version": "0.1.0",
  "description": "Automated testing application for AMWA NMOS IS-04.",
  "main": "./dist/electron/main.js",
  "scripts": {
    "build": "node .electron-vue/build.js && electron-builder",
    "build:dir": "node .electron-vue/build.js && electron-builder --dir",
    "build:clean": "cross-env BUILD_TARGET=clean node .electron-vue/build.js",
    "build:web": "cross-env BUILD_TARGET=web node .electron-vue/build.js",
    "dev": "node .electron-vue/dev-runner.js",
    "lint": "eslint --ext .js,.vue -f ./node_modules/eslint-friendly-formatter src",
    "lint:fix": "eslint --ext .js,.vue -f ./node_modules/eslint-friendly-formatter --fix src",
    "pack": "npm run pack:main && npm run pack:renderer",
    "pack:main": "cross-env NODE_ENV=production webpack --progress --colors --config .electron-vue/webpack.main.config.js",
    "pack:renderer": "cross-env NODE_ENV=production webpack --progress --colors --config .electron-vue/webpack.renderer.config.js",
    "postinstall": "npm run lint:fix",
    "test": "tape test/*.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Streampunk/zenmos.git"
  },
  "keywords": [
    "NMOS",
    "AMWA",
    "IS-04"
  ],
  "author": "Streampunk Media <furnace@streampunk.media> (http://www.streampunk.media)",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/Streampunk/zenmos/issues"
  },
  "homepage": "https://github.com/Streampunk/zenmos#readme",
  "build": {
    "productName": "Zenmos",
    "appId": "media.streampunk.electron-vue",
    "asar": false,
    "directories": {
      "output": "build"
    },
    "files": [
      "dist/electron/**/*"
    ],
    "extraFiles": [
      {
        "from": "nodes",
        "to": "resources/app/nodes",
        "filter": "**/*"
      },
      {
        "from": "schemas",
        "to": "resources/app/schemas",
        "filter": "**/*"
      },
      {
        "from": "flows",
        "to": "resources/app/flows",
        "filter": "**/*"
      },
      {
        "from": "util",
        "to": "resources/app/util",
        "filter": "**/*"
      }
    ],
    "dmg": {
      "contents": [
        {
          "x": 410,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 150,
          "type": "file"
        }
      ]
    },
    "mac": {
      "icon": "icons/icon.icns"
    },
    "win": {
      "icon": "icons/icon.ico"
    },
    "linux": {
      "icon": "icons"
    }
  },
  "dependencies": {
    "ajv": "^6.5.5",
    "body-parser": "^1.18.3",
    "digd.js": "git+https://git.coolaj86.com/scriptorian/digd.js#master",
    "dns-suite": "git+https://git.coolaj86.com/scriptorian/dns-suite.js#master",
    "express": "^4.16.4",
    "node-red": "^0.19.5",
    "request": "^2.88.0",
    "request-promise-native": "^1.0.5",
    "sdpoker": "^0.0.12",
    "uuid": "^3.3.2",
    "vue": "^2.5.17",
    "vue-electron": "^1.0.6",
    "vue-router": "^3.0.1",
    "vuetify": "1.3.7",
    "yargs": "^12.0.2"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "babili-webpack-plugin": "^0.1.2",
    "cfonts": "^2.1.2",
    "chalk": "^2.4.1",
    "copy-webpack-plugin": "^4.5.1",
    "cross-env": "^5.1.6",
    "css-loader": "^1.0.1",
    "del": "^3.0.0",
    "devtron": "^1.4.0",
    "electron": "^2.0.4",
    "electron-builder": "^20.19.2",
    "electron-debug": "^1.5.0",
    "electron-devtools-installer": "^2.2.4",
    "eslint": "^4.19.1",
    "eslint-config-google": "^0.11.0",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-html": "^4.0.3",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.8.0",
    "eslint-plugin-standard": "^3.1.0",
    "mini-css-extract-plugin": "0.4.0",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.2.0",
    "multispinner": "^0.2.1",
    "node-loader": "^0.6.0",
    "node-sass": "^4.9.2",
    "sass-loader": "^7.0.3",
    "style-loader": "^0.21.0",
    "tape": "^4.9.1",
    "url-loader": "^1.0.1",
    "vue-html-loader": "^1.2.4",
    "vue-loader": "^15.4.2",
    "vue-style-loader": "^4.1.0",
    "vue-template-compiler": "^2.5.17",
    "webpack": "^4.15.1",
    "webpack-dev-server": "^3.1.4",
    "webpack-hot-middleware": "^2.22.2",
    "webpack-merge": "^4.1.3"
  },
  "node-red": {
    "nodes": {
      "jsonValidator": "nodes/jsonValidator.js",
      "auditLog": "nodes/auditLog.js",
      "stateStoreRAM": "nodes/stateStoreRAM.js",
      "registrationLogic": "nodes/registrationLogic.js",
      "queryLogic": "nodes/queryLogic.js",
      "nmosHTTPRequest": "nodes/nmosHTTPRequest.js",
      "mdns": "nodes/mdns.js",
      "dns-sd": "nodes/dns-sd.js",
      "dnsBrowser": "nodes/dnsBrowser.js",
      "nodeLogic": "nodes/nodeLogic.js",
      "heartbeats": "nodes/heartbeats.js",
      "nodeRequester": "nodes/nodeRequester.js",
      "SDPValidator": "nodes/SDPValidator.js",
      "integrityCheck": "nodes/integrityCheck.js"
    }
  }
}
